    "cookie-parser": "~1.4.3",
    "csvtojson": "^2.0.8",
    "date-fns": "^1.30.1",
    "debug": "~2.6.9",
    "express": "~4.16.0",
    "http-errors": "~1.6.2",
    "morgan": "~1.9.0",
    "pug": "^2.0.3",
    "ts-node": "^8.1.0"
  },
  "devDependencies": {
    "@types/cookie-parser": "^1.4.1",
    "@types/http-errors": "^1.6.1",
    "@types/jest": "^24.0.12",
    "@types/node": "^11.13.8",
    "eslint": "^5.16.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-config-prettier": "^4.1.0",
    "eslint-plugin-import": "^2.17.2",
    "jest": "^24.8.0",
    "nodemon": "^1.18.11",
    "ts-jest": "^24.0.2",
    "tslint": "^5.16.0",
    "typescript": "^3.4.5"
